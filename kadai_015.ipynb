        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(f\"name: {self.name}, age: {self.age}\")\n",
        "\n",
        "person = Human(\"Shigetoshi\", 45)\n",
        "person.printinfo()\n",
